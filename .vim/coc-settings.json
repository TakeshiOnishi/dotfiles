{
  "coc.preferences.jumpCommand": "split",
  "solargraph": {
    "commandPath": "solargraph",
    "formatting": true,
    "diagnostics": true,
    "shell": "zsh"
  },
  "clangd": {
    "path": "clangd",
    "enabled": true
  },
  "bashIde.shellcheckPath": "shellcheck",
  "lists.disabledLists": [],
  "json.enable": true,
  "markdownlint.onSave": false,
  "html": {
    "filetypes": ["html"],
    "format.enable": true
  },
  "css.execArgv": [],
  "prettier.enable": true,
  "typescript.inlayHints": {
    "variableTypes.enabled": true,
    "parameterNames": {
      "enabled": "all",
      "suppressWhenArgumentMatchesName": false
    },
    "functionLikeReturnTypes.enabled": true
  },
  "python": {
    "formatting": {
      "provider": "black"
    },
    "linting": {
      "flake8Enabled": true
    }
  },
  "languageserver": {
    "terraform": {
      "command": "terraform-ls",
      "args": ["serve"],
      "filetypes": ["terraform", "tf"],
      "initializationOptions": {},
      "settings": {}
    }
  }
}
